# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Documentation play
GET     /icon                           controllers.Application.icon

GET     /doc                           controllers.Application.doc

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /css/*file                  controllers.Assets.at(path="/boostrap/theme/css", file)




# Home page
GET           /                       controllers.Application.home

GET           /how-it-works                       controllers.Application.howItWorks

GET           /company                controllers.Application.company

GET           /register                     controllers.Application.register

# get Square

#current month

GET           /get-squares/:month                      controllers.Application.haveSquares(month: String)

GET  /confirmSquares  controllers.Application.confirmSquares





